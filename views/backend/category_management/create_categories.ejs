                <form method="POST" action="/dashboard/category/create">
                    <div class="card">
                        <div class="card-header">
                            <div class="d-flex justify-content-between">
                                <h3>

                                    Create
                                </h3>
                                <div>
                                    <a class="btn btn-sm btn-outline-success" href="/dashboard/category">
